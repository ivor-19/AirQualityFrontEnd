{
  "expo": {
    "name": "Air Guard",
    "slug": "air-guard",
    "version": "5.1.0",
    "orientation": "portrait",
    "icon": "./assets/app-icons/leaf-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/app-icons/leaf-adaptive-icon.png"
      },
      "package": "com.rov19.client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/leaf-128.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/app-icons/leaf-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain"
        }
      ],
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "018aa1b4-281b-4d7d-ba42-8257a18205f6"
      }
    }
  }
}
