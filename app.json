{
  "expo": {
    "name": "Air Guard",
    "slug": "air_guard",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/icons/leaf.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/leaf.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rov19.client"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/icons/leaf.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/icons/leaf.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "e91d08e2-e012-4d60-81ec-eb6f1d277d46"
      }
    }
  }
}
